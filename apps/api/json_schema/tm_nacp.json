{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Торговельна марка",
  "description": "Опис даних API СІС",
  "type": "object",
  "properties": {
    "obj_type": {
      "type": "string",
      "description": "Тип об'єкта промислової власності"
    },
    "obj_type_id": {
      "type": "integer",
      "description": "Ідентифікатор об’єкта промислової власності"
    },
    "obj_state": {
      "type": "integer",
      "description": "Ідентифікатор стану об’єкта промислової власності (1 - заявка, 2 - охоронний документ)"
    },
    "app_number": {
      "type": "string",
      "description": "Номер заявки"
    },
    "app_date": {
      "type":  ["string", "null"],
      "format": "date-time",
      "description": "Дата подання заявки"
    },
    "registration_number": {
      "type": ["string", "null"],
      "description": "Номер охоронного документа"
    },
    "registration_date": {
      "type": ["string", "null"],
      "format": "date-time",
      "description": "Дата видачі охоронного документа"
    },
    "last_update": {
      "type": "string",
      "format": "date-time",
      "description": "Дата останньої зміни запису"
    },
    "data": {
      "description": "Бібліографічні дані",
      "type": "object",
      "properties": {
        "ApplicantDetails": {
          "description": "Дані щодо заявника",
          "type": "object",
          "properties": {
            "Applicant" : {
              "description": "Список заявників",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "ApplicantAddressBook": {
                    "type": "object",
                    "properties": {
                      "FormattedNameAddress": {
                        "description": "Дані щодо адреси та найменування заяника",
                        "type": "object",
                        "properties": {
                          "Address": {
                            "type": "object",
                            "properties": {
                              "AddressCountryCode": {
                                "description": "Код країни",
                                "type": "string"
                              },
                              "FreeFormatAddress": {
                                "type": "object",
                                "properties": {
                                  "FreeFormatAddressLine": {
                                    "description": "Адреса заявника",
                                    "type": "string"
                                  },
                                  "FreeFormatAddressLineOriginal": {
                                    "description": "Адреса заявника мовою оригіналу",
                                    "type": "string"
                                  },
                                  "FreeFormatNameLineOriginal": {
                                    "description": "Найменування заявника мовою оригіналу",
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          },
                          "Name": {
                            "description": "Дані щодо найменування заявника",
                            "type": "object",
                            "properties": {
                              "FreeFormatName": {
                                "type": "object",
                                "properties": {
                                  "EDRPOU": {
                                    "description": "ЄДРПОУ",
                                    "type": "string"
                                  },
                                  "FreeFormatNameDetails": {
                                    "type": "object",
                                    "properties": {
                                      "FreeFormatNameLine": {
                                        "description": "Найменування заявника",
                                        "type": "string"
                                      },
                                      "FreeFormatNameLineOriginal": {
                                        "description": "Найменування заявника мовою оригіналу",
                                        "type": "string"
                                      }
                                    }
                                  },
                                  "NameKind": {
                                    "description": "Тип суб'єкта (legal_entity, Legal Entity - юр. особа, individual, Natural Person - фіз. особа, Other - інше)",
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  },
                  "ApplicantSequenceNumber": {
                    "description": "Порядковий номер суб'єкта у списку заявників",
                    "type": ["integer", "string"]
                  }
                }
              }
            }
          }
        },
        "CorrespondenceAddress": {
          "description": "Дані щодо адреси для листування",
          "type": "object",
          "properties": {
            "CorrespondenceAddressBook": {
              "type": "object",
              "properties": {
                "Address": {
                  "description": "Дані щодо адреси",
                  "type": "object",
                  "properties": {
                    "AddressCountryCode": {
                      "description": "Код країни",
                      "type": "string"
                    },
                    "AddressPostcode": {
                      "description": "Поштовий індекс",
                      "type": "string"
                    },
                    "FreeFormatAddressLine": {
                      "description": "Адреса",
                      "type": "string"
                    }
                  }
                },
                "Name": {
                  "description": "Дані щодо найменування",
                  "type": "object",
                  "properties": {
                    "FreeFormatNameLine": {
                      "description": "Найменування суб'єкта",
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "HolderDetails": {
          "description": "Дані щодо власника охоронного документа",
          "type": "object",
          "properties": {
            "Holder": {
              "description": "Список власників",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "HolderAddressBook": {
                    "type": "object",
                    "properties": {
                      "FormattedNameAddress": {
                        "description": "Дані щодо адреси та найменування власника",
                        "type": "object",
                        "properties": {
                          "Address": {
                            "description": "Дані щодо адреси",
                            "type": "object",
                            "properties": {
                              "AddressCountryCode": {
                                "description": "Код країни",
                                "type": "string"
                              },
                              "FreeFormatAddress": {
                                "type": "object",
                                "properties": {
                                  "FreeFormatAddressLine": {
                                    "description": "Адреса власника",
                                    "type": "string"
                                  },
                                  "FreeFormatAddressLineOriginal": {
                                    "description": "Адреса власника мовою оригінала",
                                    "type": "string"
                                  },
                                  "FreeFormatNameLine": {
                                    "description": "Найменування власника",
                                    "type": "string"
                                  },
                                  "FreeFormatNameLineOriginal": {
                                    "description": "Найменування власника мовою оригінала",
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          },
                          "Name": {
                            "description": "Дані щодо найменування",
                            "type": "object",
                            "properties": {
                              "FreeFormatName": {
                                "type": "object",
                                "properties": {
                                  "EDRPOU": {
                                    "description": "Код ЄДРПОУ",
                                    "type": "string"
                                  },
                                  "FreeFormatNameDetails": {
                                    "type": "object",
                                    "properties": {
                                      "FreeFormatNameLine": {
                                        "description": "Адреса власника",
                                        "type": "string"
                                      },
                                      "FreeFormatNameLineOriginal": {
                                        "description": "Адреса власника мовою оригінала",
                                        "type": "string"
                                      }
                                    }
                                  },
                                  "NameKind": {
                                    "description": "Тип суб'єкта (legal_entity, Legal Entity - юр. особа, individual, Natural Person - фіз. особа, Other - інше)",
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  },
                  "HolderSequenceNumber": {
                    "description": "Порядковий номер суб'єкта у списку власників",
                    "type": ["integer", "string"]
                  }
                }
              }
            }
          }
        },
        "MarkImageDetails": {
          "description": "Дані щодо зображення",
          "type": "object",
          "properties": {
            "MarkImage": {
              "type": "object",
              "properties": {
                "MarkImageCategory": {
                  "description": "Віденська класифікація",
                  "type": "object",
                  "properties": {
                    "CategoryCodeDetails": {
                      "type": "object",
                      "properties": {
                        "CategoryCode": {
                          "description": "Список класів Віденської класифікації",
                          "type": "array",
                          "items": {
                            "type": ["string", "null"]
                          }
                        }
                      }
                    }
                  }
                },
                "MarkImageColourClaimedText": {
                  "description": "Список заявлених кольорів",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "#text": {
                        "description": "Найменування кольору",
                        "type": "string"
                      },
                      "@sequenceNumber": {
                        "description": "Порядковий номер кольору у списку",
                        "type": ["integer", "string"]
                      }
                    }
                  }
                },
                "MarkImageColourIndicator": {
                  "description": "Зазначення того що зображення кольорове (true, false)",
                  "type": "string"
                },
                "MarkImageFileFormat": {
                  "description": "Формат зображення",
                  "type": "string"
                },
                "MarkImageFilename": {
                  "description": "Відносне посилання на зображення",
                  "type": "string"
                },
                "MarkImageRepresentationSize": {
                  "description": "Технічні дані зображення",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "MarkImageRenditionColourMode": {
                        "type": "string"
                      },
                      "MarkImageRenditionRepresentationSize": {
                        "type": "object",
                        "properties": {
                          "Height": {
                            "type": ["integer", "string"]
                          },
                          "Resolution": {
                            "type": "integer"
                          },
                          "ResolutionUnit": {
                            "type": "string"
                          },
                          "Unit": {
                            "type": "string"
                          },
                          "Width": {
                            "type":  ["integer", "string"]
                          }
                        }
                      }
                    }
                  }
                },
                "MarkImageTypeNotice": {
                  "description": "Ознака того чи містить зображення неприпустимі символи (якщо не пусте, то містить)",
                  "type": "string"
                }
              }
            }
          }
        },
        "RepresentativeDetails": {
          "description": "Дані щодо представника",
          "type": "object",
          "properties": {
            "Representative": {
              "description": "Список представників",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "RepresentativeAddressBook": {
                    "type": "object",
                    "properties": {
                      "FormattedNameAddress": {
                        "description": "Дані щодо найменування та адреси представника",
                        "type": "object",
                        "properties": {
                          "Address": {
                            "description": "Дані щодо адреси представника",
                            "type": "object",
                            "properties": {
                              "AddressCountryCode": {
                                "description": "Код країни",
                                "type": "string"
                              },
                              "FreeFormatAddress": {
                                "type": "object",
                                "properties": {
                                  "FreeFormatAddressLine": {
                                    "description": "Адреса представника",
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          },
                          "Name": {
                            "description": "Дані щодо найменування представника",
                            "type": "object",
                            "properties": {
                              "FreeFormatName": {
                                "type": "object",
                                "properties": {
                                  "FreeFormatNameDetails": {
                                    "type": "object",
                                    "properties": {
                                      "FreeFormatNameDetails": {
                                        "type": "object",
                                        "properties": {
                                          "FreeFormatNameLine": {
                                            "description": "Найменування представника",
                                            "type": "string"
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "WordMarkSpecification": {
          "description": "Дані щодо ключових слів",
          "type": ["object", "null"],
          "properties": {
            "MarkSignificantVerbalElement": {
              "description": "Список ключових слів",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "#text": {
                    "description": "Ключове слово",
                    "type": "string"
                  },
                  "@sequenceNumber": {
                    "description": "Порядок ключового слова у списку",
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
